{
    "$schema": "../../../node_modules/@angular-devkit/schematics/collection-schema.json",
    "schematics": {
        "ng-add": {
            "description":"Adds ngx-start-setup-schematics library o project",
            "factory":"./ng-add/index#ngAdd"
        },
        "add-tailwind" : {
            "description":"Installs and configures TailwindCSS",
            "factory":"./add-tailwind/index#addTailwind"
        },
        "add-generic-providers" : {
            "description":"Adds generic use providers",
            "factory":"./add-material/index#addGenericProviders"
        },
        "add-material" : {
            "description":"Installs and configures Angular Material",
            "factory":"./add-material/index#addMaterial"
        },
        "customize-entry-page" : {
            "description":"Updates entry page",
            "factory":"./customize-entry-page/index#customizeEntryPage",
            "schema": "./customize-entry-page/schema.json",
            "aliases": ["finalize"]
        }

    }
}